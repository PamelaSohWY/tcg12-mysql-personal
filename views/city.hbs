{{#extends 'base'}}

{{#block 'content'}}
<h1> City</h1>

<table class="table">
<thead>
    <tr>
        <th>City Id</th>
        <th>City</th>
        <th>Country Id</th>
        <th>Last Update</th>
    </tr>
</thead> 

{{#each city}}
    <tr>
        <td>{{this.city_id}}</td>
        <td>{{this.city}}</td>
        <td>{{this.country_id}}</td>
        <td>{{this.last_update}}</td>
    </tr>
{{/each}}
</table>

{{/block}}
{{/extends}}